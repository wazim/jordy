{
  "manifest_version": 2,
  "name": "Jordy",
  "description": "Project Jordan Chrome Extension",
  "version": "1.1",
  "background": {
    "scripts": ["jquery-2.1.4.min.js", "background.js"]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Jordan"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://project-jordan.herokuapp.com/jordan/",
    "notifications",
    "storage"
  ]
}
